<addon id="plugin.video.wwmediavod"
       name="WW Media VOD"
       version="1.0"
       provider-name="sClarke">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.requests" />
	<import addon="script.module.metahandler"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">WW Media VOD</summary>
    <language>en</language>
    <description lang="en">WW Media VOD Official Addon - Supplier of Great Quality and Stable VOD Content</description>
